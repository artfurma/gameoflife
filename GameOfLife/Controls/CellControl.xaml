<UserControl x:Class="GameOfLife.Controls.CellControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GameOfLife.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="32" Name="GoLCell">
	<UserControl.Resources>
		<ResourceDictionary Source="Styles.xaml" />
	</UserControl.Resources>
	<Border Style="{StaticResource CellControlStyle}" Background="{Binding ElementName=GoLCell, Path=Background}" />
</UserControl>