<UserControl x:Class="GameOfLifeControls.Cell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GameOfLifeControls"
             mc:Ignorable="d"
             d:DesignHeight="32" d:DesignWidth="32" Name="CellControl">
	<UserControl.Resources>
		<ResourceDictionary Source="Styles.xaml" />
	</UserControl.Resources>
	<Border Style="{StaticResource CellStyle}" Background="{Binding ElementName=CellControl, Path=Background, FallbackValue=White}" />
</UserControl>