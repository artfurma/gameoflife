<UserControl x:Class="GameOfLifeControls.GameBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GameOfLifeControls"
             mc:Ignorable="d"
             d:DesignHeight="400" d:DesignWidth="780" Name="GameBoardControl">
	<UserControl.Resources>
		<ResourceDictionary Source="Styles.xaml" />
	</UserControl.Resources>
	<Grid x:Name="GameGrid" Style="{StaticResource GameBoardStyle}"/>
</UserControl>